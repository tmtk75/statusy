{
  "name": "statusy",
  "version": "0.0.11",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --progress --colors",
    "build:watch": "webpack --watch --progress --colors",
    "build:mkicon": "./bin/mkicon.sh ./dist/myicon ./assets/icon.png",
    "release-build": "npm run distclean && npm run build && npm run build:mkicon && make archive",
    "kii:build": "webpack --progress --colors --config ./src/kiicloud/webpack.config.js",
    "kii:build:watch": "webpack --watch --progress --colors --config ./src/kiicloud/webpack.config.js",
    "kii:deploy-code": "kii-cli servercode deploy dist/kii-servercode.js",
    "kii:deploy-hook": "kii-cli servercode hook-attach src/kiicloud/hook.json",
    "kii:deploy": "npm run kii:deploy-code && npm run kii:deploy-hook",
    "kii:build-deploy": "npm run kii:build && npm run kii:deploy",
    "distclean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/electron": "^1.4.25",
    "@types/material-ui": "^0.16.41",
    "@types/react": "^0.14.50",
    "@types/react-addons-css-transition-group": "^0.14.18",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.33",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^0.8.33",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.0",
    "electron": "^1.4.8",
    "electron-load-devtool": "^0.4.0",
    "electron-packager": "^8.2.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.16.1",
    "moment": "^2.17.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-observable": "^0.12.1",
    "rxjs": "^5.0.0-rc.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "ts-loader": "^1.1.0",
    "typescript": "^2.1.1",
    "webpack": "^1.13.3"
  }
}
